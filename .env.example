# Domain Configuration
DOMAIN=your-domain.com

# Traefik Configuration
# IMPORTANT: Generate using: htpasswd -nb admin yourpassword | sed 's/\$/\$\$/g'
TRAEFIK_BASIC_AUTH=REPLACE_WITH_HTPASSWD_HASH

# PostgreSQL Configuration
POSTGRES_DB=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=change_this_strong_password_123!
LITELLM_DB=litellm
OPENWEBUI_DB=openwebui

# Redis Configuration
REDIS_PASSWORD=change_this_redis_password_456!

# LiteLLM Configuration
LITELLM_MASTER_KEY=sk-1234567890abcdef  # Your master API key for LiteLLM
LITELLM_SALT_KEY=change_this_salt_key_789!  # Used for encrypting keys in DB
LITELLM_UI_USERNAME=admin
LITELLM_UI_PASSWORD=change_this_ui_password!

# Open WebUI Configuration
WEBUI_SECRET_KEY=change_this_secret_key_very_long_and_random_string_012!
ENABLE_SIGNUP=false  # Set to true if you want to allow user registration
DEFAULT_USER_ROLE=user  # Options: user, admin
WEBUI_NAME=AI Assistant Platform

# Model Configuration
MODEL_FILTER_LIST=gpt-4,gpt-3.5-turbo,claude-3-opus,claude-3-sonnet  # Comma-separated list
TASK_MODEL=gpt-3.5-turbo
TITLE_MODEL=gpt-3.5-turbo

# Optional: Webhook for notifications
WEBHOOK_URL=

# Optional: Search Query Generation Prompt
SEARCH_PROMPT_TEMPLATE=Generate a concise and focused web search query based on the user's question or conversation context. Return only the search query.

# Email Configuration (Optional - for notifications)
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
SMTP_FROM=

# API Keys for LiteLLM (Add your provider keys)
OPENAI_API_KEY=sk-your-openai-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key
AZURE_API_KEY=
AZURE_API_BASE=
AZURE_API_VERSION=
COHERE_API_KEY=
HUGGINGFACE_API_KEY=

# Monitoring (Optional)
ENABLE_PROMETHEUS=false
ENABLE_JAEGER=false

# Backup Configuration
BACKUP_KEEP_DAYS=7
BACKUP_S3_BUCKET=
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
